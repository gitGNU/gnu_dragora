echo "Uncompressing structure of directories ..."
if [ ! -f ${ARCHIVE}/_hierarchyfs.tar.gz ]; then
  echo "${ARCHIVE}/_hierarchyfs.tar.gz is not a regular file."
  exit 1
fi
cd $ROOT && tar xf ${ARCHIVE}/_hierarchyfs.tar.gz

